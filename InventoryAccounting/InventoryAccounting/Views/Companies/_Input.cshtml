@model InventoryAccounting.Models.DB.Companies

@{
    Layout = null;
}
<form asp-action="@ViewBag.Action" id="form">
    <input name="IsValid" type="hidden" value="@ViewData.ModelState.IsValid.ToString()" />
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="Unp" class="col-form-label"></label>
        <input asp-for="Unp" class="form-control"/>
        <span asp-validation-for="Unp" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Name" class="col-form-label"></label>
        <input asp-for="Name" class="form-control"/>
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Address" class="col-form-label"></label>
        <input asp-for="Address" class="form-control"/>
        <span asp-validation-for="Address" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="DirectorsName" class="col-form-label"></label>
        <input asp-for="DirectorsName" class="form-control"/>
        <span asp-validation-for="DirectorsName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="DirectorsPhone" class="col-form-label"></label>
        <input asp-for="DirectorsPhone" class="form-control"/>
        <span asp-validation-for="DirectorsPhone" class="text-danger"></span>
    </div>
</form>
                        


