@model IEnumerable<InventoryAccounting.Models.DB.Contracts>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="modal-placeholder"></div>

<div class="row">
    <div class="container-fluid my-2">
        <h2 class="d-inline">Контракты</h2>
        <a asp-action="Create" class="d-inline float-right text-dark fas fa-plus-circle fa-2x"></a>
    </div>
    <div class="container-fluid" id="partialTable">
        @await Html.PartialAsync("_Table", Model)
    </div>
</div>